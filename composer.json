{
  "name": "tardigrades/sexy-field",
  "description": "SexyField helps you build application structure and accompanying forms.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Dion Snoeijen",
      "email": "hallo@dionsnoeijen.nl"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.5.6",
    "symfony/console": "^3.3.2",
    "symfony/form": "^3.3.2",
    "symfony/validator": "^3.3.2",
    "symfony/security": "^3.3.2",
    "symfony/dependency-injection": "^3.3.2",
    "symfony/inflector": "^3.3.2",
    "symfony/yaml": "^3.3.2",
    "symfony/config": "^3.3.2",
    "symfony/event-dispatcher": "^3.3.2",
    "beberlei/assert": "2.6.5",
    "twig/twig": "^2.0",
    "twig/extensions": "^1.5.1",
    "php-coveralls/php-coveralls": "^1.0",
    "symfony/cache": "^3.4",
    "ezyang/htmlpurifier": "^4.10"
  },
  "autoload": {
    "psr-4": {
      "Tardigrades\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tardigrades\\": "test/unit/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "mockery/mockery": "^0.9.9",
    "mikey179/vfsStream": "^1.6",
    "phing/phing": "^2.16",
    "squizlabs/PHP_CodeSniffer": "^3.1"
  }
}
